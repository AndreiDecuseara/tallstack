{
  "name": "andreidecuseara/tallstack",
  "description": "Full install of tallstack",
  "keywords": [
    "alpine",
    "alpinejs",
    "tailwind",
    "tailwindcss",
    "tailwindui",
    "tall",
    "laravel",
    "livewire",
    "preset"
],
  "homepage": "https://github.com/AndreiDecuseara/tallstack.git",
  "license": "MIT",
  "authors": [
    {
        "name": "Andrei Decuseara",
        "email": "adecuseara@gmail.com"
    }
],

"require": {
  "php": "^7.2.5|^8.0",
  "laravel/ui": "^3.0",
  "illuminate/support": "^8.0"
},

"autoload": {
  "psr-4": {
      "TallStackPackage\\TallStack\\": "src/"
  }
},
  "extra": {
    "laravel": {
        "providers": [
            "TallStackPackage\\TallStack\\TallStackServiceProvider"
        ]
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}

